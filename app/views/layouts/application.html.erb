<!DOCTYPE html>
<html>
<head>
  <title>CalendarFun</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>

<%- flash.each do |name, msg| %>
  <div id="flash_<%= name %>"><%= msg %></div>
<%- end %>

<%= yield %>

</body>
</html>
